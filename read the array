#include<stdio.h>
int main()
{
    int n,i;
    int arr[100];
    printf("enter the number of elements(1-100):");
    scanf("%d",&n);
    if(n<1||n>100)
    {
        printf("invalid number of elements.\n",n);
        for(i=0;i<n;i++){
            scanf("%d",&arr[i]);
        }
        printf("you entered:\n");
        for(i=0;i<n;i++)
        {
            printf("%d",arr[i]);
        }
        return 0;
}

