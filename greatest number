#include<stdio.h>
int num1,num2,num3,greatest;
int main()
{
    printf("enter frist number");
    scanf("%d",&num1);
    printf("enter second number");
    scanf("%d",&num2);
    printf("enter third number");
    scanf("%d",&num3);
    greatest=num1;
    if(num2>greatest)
    {
        greatest=num2;
    }
if(num3>greatest)
{
    greatest=num3;
}
printf("the greatest number is:%d\n",greatest);
return 0;
}
